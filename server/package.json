{
  "name": "connection-manager-api",
  "version": "1.8.14",
  "description": "ModusBox Connection Manager API",
  "main": "index.js",
  "scripts": {
    "testX": "standardx --verbose | snazzy && mocha",
    "test": "./run-vault.sh && mocha ; ./kill-vault.sh",
    "test-self-signed-server": "./run-vault.sh && TEST_START_SELF_SIGNED_SERVER=true npm test ; ./kill-vault.sh",
    "wtftest": "wtfnode ./node_modules/.bin/_mocha",
    "test-with-coverage": "./run-vault.sh && nyc --reporter=text mocha --timeout 20000 --colors -exit ; ./kill-vault.sh",
    "start": "node src/index.js",
    "start-with-oauth-enabled": "source oauth.env && node src/index.js",
    "start-for-tips": "source ../localEnvs/tips.env  && node src/index.js",
    "build": "babel src --out-dir ./dist --source-maps",
    "serve": "node ./dist/index.js",
    "migrate": "DOTENV_CONFIG_PATH=$(pwd)/.env npx knex --knexfile src/knexfile.js migrate:latest",
    "seed": "DOTENV_CONFIG_PATH=$(pwd)/.env npx knex --knexfile src/knexfile.js seed:run",
    "migrate-and-seed": "npm -s run migrate && npm -s run seed"
  },
  "keywords": [
    "swagger"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@kubernetes/client-node": "^0.16.2",
    "async-retry": "^1.3.3",
    "chai": "^4.3.6",
    "connect": "^3.7.0",
    "cookies": "^0.8.0",
    "cors": "^2.8.5",
    "dotenv": "^15.0.0",
    "env-var": "^7.1.1",
    "express-winston": "^4.2.0",
    "form-data": "^4.0.0",
    "joi": "^17.6.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.2",
    "moment": "^2.29.1",
    "mysql2": "^2.3.3",
    "node-forge": "^1.2.1",
    "node-vault": "^0.9.22",
    "oas3-tools": "^2.2.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "sinon": "^13.0.1",
    "snazzy": "^9.0.0",
    "soap": "^0.43.0",
    "winston": "^3.5.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "jshint": "^2.13.4",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "standardx": "^7.0.0"
  },
  "standardx": {
    "semistandard": true
  }
}
